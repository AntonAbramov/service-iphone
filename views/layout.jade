      #formstep1 p, #formstep2 a, #formstep2 p, #formstep3 a, #formstep3 p, #formstep4 p, .benefits__heading, .benefits__list li .aa, .benefits__list li:before, .blue-number, .btn, .device-title, .devices, .devices__item, .devices__name, .didnt-find, .fa-fw, .fa-li, .fa-stack-1x, .fa-stack-2x, .header-section__schedule, .heading-title, .lp-header__telephone, .modal-heading, .nav__link, .text-center {
