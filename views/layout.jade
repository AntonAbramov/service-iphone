        -moz-osx-font-smoothing: grayscale
      }

      .container-fluid:after, .container:after, .row:after {
        clear: both
      }

      .comments-tree, .lp-header__telephone, .problems__item {
        list-style: none
      }

      .devices__link:focus, .devices__link:hover, .didnt-find__telephone, .footer-link, .header-section__contacts__tel, .header-section__schedule--description, .nav__link, .problems__link, .site-logo__link, .top-navigation__link {
        text-decoration: none
