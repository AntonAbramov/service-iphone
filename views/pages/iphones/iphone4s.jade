        .row.pricelist__row
          .col-md-5
            .pricelist__item__title Замена нижнего шлейфа
            time.pricelist__item__time 
          .col-md-3
            .pricelist__item__cost 350 грн.
          .col-md-4
            .pricelist__item__description
